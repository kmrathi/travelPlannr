    <servlet>
        <servlet-name>testServlet</servlet-name>
        <servlet-class>com.trip.plannr.servlet.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testServlet</servlet-name>
        <url-pattern>/testServlet</url-pattern>
    </servlet-mapping>

    </web-app>